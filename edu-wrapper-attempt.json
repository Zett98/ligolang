{
  "name": "ligo-wrapper",
  "version": "0.1.0",
  "description": "Example Reason Esy Project",
  "license": "MIT",
  "esy": {
    "build": [
      "dune build -p #{self.name}",
      "cp -nf #{self.target_dir / 'install' / 'default' / 'bin' / 'Ligo_wrapper'} #{self.root}/"
    ],
    "buildDev": [
      "refmterr dune build --promote-install-files --root . --only-package #{self.name}",
      "cp -nf #{self.target_dir / 'install' / 'default' / 'bin' / 'Ligo_wrapper'} #{self.root}/"
    ],
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": {
      "rewritePrefix": true,
      "bin": [
        "Ligo_wrapper"
      ]
    },
    "buildEnv": {
      "ODOC_SYNTAX": "re"
    }
  },
  "scripts": {
    "test": "esy x Ligo_wrapper",
    "format": "esy dune build @fmt --auto-promote",
    "doc": "esy dune build @doc"
  },
  "dependencies": {
    "@opam/dune": "*",
    "ocaml": "4.10.x",
    "@opam/ligo": "*"
  },
  "devDependencies": {
    "@opam/ocaml-lsp-server": "*"
  },
  "resolutions": {
    "@opam/ligo-types": "github:anchpop/ligolang:ligo-types.opam#7dee617edeff9240daced4fb78330904f8f49492",
    "@opam/ligo": {
      "source": "github:anchpop/ligolang:ligo.opam#7dee617edeff9240daced4fb78330904f8f49492",
      "override": {
        "dependencies": {
          "@opam/coq": null
        }
      }
    },
    "@opam/bls12-381-unix": {
      "source": "git:https://gitlab.com/dannywillems/ocaml-bls12-381.git:bls12-381-unix.opam#d448d9d370ae711d9357bf0e366281b711f4b422",
      "override": {
        "buildEnv": {
          "OPAM_SWITCH_PREFIX": "#{ @opam/tezos-rust-libs.install }"
        },
        "dependencies": {
          "@opam/bls12-381": "0.4.3",
          "@opam/bls12-381-gen": "0.4.3",
          "@opam/ctypes": "*"
        }
      }
    }
  }
}
